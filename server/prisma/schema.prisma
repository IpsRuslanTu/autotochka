generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url  = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model WorkDay {
  id        Int    @id @default(autoincrement())
  date      DateTime @unique @db.Date
  isAvailable Boolean @default(true)
}
